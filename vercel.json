{
  "builds": [
    {
      "src": "wedding_album_project/elevate/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wedding_album_project/elevate/wsgi.py"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
